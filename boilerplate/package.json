{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "babelrc": "node other/generate-babelrc.js",
    "clean:babelrc": "rimraf .babelrc",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:open": "start-server-and-test dev :3000 cy:open",
    "pretest:e2e:run": "npm run babelrc",
    "test:e2e:run": "cross-env CODE_COVERAGE=true start-server-and-test dev :3000 cy:run",
    "posttest:e2e:run": "npm run clean:babelrc",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^14.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cypress/code-coverage": "^3.10.0",
    "@next/bundle-analyzer": "^13.0.3",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^11.0.1",
    "esbuild": "^0.15.14",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.48.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.8.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
